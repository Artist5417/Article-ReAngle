{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.py" },
    { "source": "/process", "destination": "/api/index.py" },
    { "source": "/static/(.*)", "destination": "/frontend/$1" },
    { "source": "/(.*)", "destination": "/frontend/index.html" }
  ]
}


